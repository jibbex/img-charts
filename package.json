{
    "name": "img-charts",
    "version": "0.1.1",
    "description": "High-level abstraction for creating, managing, and exporting ECharts visualizations",
    "keywords": [
        "img",
        "chart",
        "image",
        "data",
        "array"
    ],
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsup src/ --format cjs,esm --dts --minify",
        "test": "jest",
        "lint": "eslint src --ext .ts,.tsx",
        "format": "prettier --write \"src/**/*.{ts,tsx}\""
    },
    "peerDependencies": {
       "echarts": "5.6.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.20.0",
        "@swc/core": "^1.10.15",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^9.20.0",
        "eslint-plugin-react": "^7.37.4",
        "globals": "^15.14.0",
        "jest": "^29.0.0",
        "prettier": "^2.0.0",
        "tsup": "^7.0.0",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.23.0"
    },
    "engines": {
        "node": ">=14.0.0",
        "bun": ">=1.2.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jibbex/imgchart.git"
    },
    "bugs": {
        "url": "https://github.com/jibbex/imgchart/issues"
    },
    "homepage": "https://jibbex.github.io/img-charts/",
    "author": "Manfred Michaelis",
    "license": "MIT",
    "sideEffects": false,
    "private": false,
    "publishConfig": {
        "access": "public"
    }
}